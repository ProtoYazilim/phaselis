{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "../*"
      ],
      "@phaselis/components": [
        "../src/components"
      ],
      "@phaselis/components/*": [
        "../src/components/*"
      ],
      "@phaselis/utils/*": [
        "../src/utils/*"
      ],
      "@phaselis/utils": [
        "../src/utils"
      ],
      "@phaselis/hooks/*": [
        "../src/hooks/*"
      ],
      "@phaselis/hooks": [
        "../src/hooks"
      ],
      "@phaselis/hocs/*": [
        "../src/hocs/*"
      ],
      "@phaselis/hocs": [
        "../src/hocs"
      ],
      "@phaselis/theme/*": [
        "../src/theme/*"
      ],
      "@phaselis/theme": [
        "../src/theme"
      ],
      "@phaselis/types/*": [
        "../src/types/*"
      ],
      "@phaselis/types": [
        "../src/types"
      ],
      "@appSrc/*": [
        "../appSrc/*"
      ],
      "@appSrc": [
        "../appSrc"
      ],
    },
  },
  "include": [
    "./**/*",
    "../appSrc/**/*",
    "../global.d.ts"
  ]
}