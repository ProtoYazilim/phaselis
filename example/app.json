{
  "expo": {
    "name": "phaselis",
    "slug": "phaselis-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.proto.phaselis"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.proto.phaselis"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images",
            "./assets/fonts",
            "./assets/logos"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "stories": "internal",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "58f85575-017b-4a93-9de4-dab126ae5129"
      }
    },
    "owner": "proto-software"
  }
}