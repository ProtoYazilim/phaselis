{
  "expo": {
    "name": "phaselis",
    "slug": "phaselis-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios_light.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.proto.phaselis",
      "icon": {
        "dark": "./assets/icons/ios_dark.png",
        "light": "./assets/icons/ios_light.png",
        "tinted": "./assets/icons/ios_tinted.png"
      },
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.proto.phaselis"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      // "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/icons/splash_icon.png",
          "dark": {
            "image": "./assets/icons/splash_icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 300
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images",
            "./assets/fonts",
            "./assets/logos",
            "./assets/icons"
          ]
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "proto-software",
          "project": "phaselis",
          "url": "https://sentry.io/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "stories": "internal",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "58f85575-017b-4a93-9de4-dab126ae5129"
      }
    },
    "owner": "proto-software"
  }
}