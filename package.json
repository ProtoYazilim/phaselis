{
  "name": "phaselis",
  "version": "0.0.10",
  "main": "expo-router/entry",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "main": "dist/index.js",
      "import": "dist/index.js",
      "require": "dist/index.js",
      "types": "dist/index.d.ts"
    }
  },
  "scripts": {
    "start": "expo start --dev-client || npx ts-node ./scripts/generate-theme-interface.ts",
    "start-no-cache": "expo start -c",
    "patch-all": "patch-package",
    "generate-theme-interface": "npx ts-node ./scripts/generate-theme-interface.ts",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios-device": "expo run:ios --device",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "cloud-build:android": "eas build --platform android --profile development",
    "cloud-build:ios": "eas build --platform ios --profile development",
    "cloud-build:ios-simulator": "eas build --platform ios --profile ios-simulator",
    "build": "rollup -c --bundleConfigAsCjs",
    "prepublishOnly": "node scripts/prepublish.js",
    "postpublish": "node scripts/postpublish.js",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "^4.5.2",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.0.2",
    "color": "^4.2.3",
    "date-fns": "^3.6.0",
    "expo": "^52.0.0",
    "expo-status-bar": "~1.12.1",
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.379.0",
    "patch-package": "^8.0.0",
    "react": "*",
    "react-native": "*",
    "react-native-awesome-slider": "^2.5.3",
    "react-native-date-picker": "^5.0.7",
    "react-native-fast-shadow": "^0.1.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mask-input": "^1.2.3",
    "react-native-picker-select": "^9.1.3",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-svg": "15.8.0",
    "react-native-unistyles": "^2.7.2",
    "react-native-web": "~0.19.10",
    "react-native-web-linear-gradient": "^1.1.2"
  },
  "peerDependencies": {
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-community/cli": "^15.1.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@types/color": "^4.2.0",
    "@types/hoist-non-react-statics": "^3.3.5",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/react": "~18.3.12",
    "@typescript-eslint/parser": "^8.18.2",
    "chokidar": "^4.0.3",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "prettier": "^3.3.3",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "*"
  },
  "private": false,
  "homepage": "https://www.protoyazilim.com/phaselis",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProtoYazilim/phaselis.git"
  }
}