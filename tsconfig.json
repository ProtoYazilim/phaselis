{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "declaration": true,
    "strict": true,
    "noEmitOnError": true,
    "paths": {
      "phaselis": [
        "./src"
      ],
      "phaselis/*": [
        "./src/*"
      ],
      "appSrc/*": [
        "./appSrc/*"
      ],
      "appSrc": [
        "./appSrc"
      ],
    },
    "outDir": "./dist/@types",
    "esModuleInterop": true
  },
  "include": [
    "src/**/*",
    "appSrc/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "global.d.ts"
  ]
}